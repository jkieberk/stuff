html, body {
  background-color: #9FBCBF;
  font-family: 'Lato', sans-serif;

}

table{
  width: 100%;
  border-collapse:collapse;
}
tr{
  border-radius: 3px;
  font-size: 22px;
  width: 90%;
  background-color: white;
}

td{
  padding: 20px;
}

table a{
  text-decoration:none;
}
